/* spellchecker:disable */
#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/outputs.h>
#include <dt-bindings/zmk/mouse.h>

// #define ZMK_MOUSE_DEFAULT_MOVE_VAL 800  // 600
// #define ZMK_MOUSE_DEFAULT_SCRL_VAL 20    // 10

&lt {
    flavor = "hold-preferred";
};

&sk {
    release-after-ms = <4000>;
    quick-release;
    lazy;
};

/ {
    chosen {
        zmk,charmap = &charmap_us;
    };
    behaviors {
        // mmv {
        //     acceleration-exponent = <0>;      // 1
        //     time-to-max-speed-ms = <900>;    // 40
        //     delay-ms = <0>;                   // 0
        // };
        // msc {
        //     acceleration-exponent = <1>;      // 0
        //     time-to-max-speed-ms = <400>;      // 500
        //     delay-ms = <0>;                   // 10
        // };
        lsk: layer_stick_key {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&mo>, <&sk>;
        };   
        lsl: layer_stick_layer {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&mo>, <&sl>;
        };   
        lcw: layer_capsword {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "hold-preferred";
            bindings = <&mo>, <&caps_word>;      
        };
        hs: hold_stick {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&kp>, <&sk>;
        };      
        hmk: hold_mouse_key {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&kp>, <&mkp>;
        };
        hcw: hold_capsword {
            compatible = "zmk,behavior-hold-tap";
            #binding-cells = <2>;
            flavor = "hold-preferred";
            tapping-term-ms = <200>;
            bindings = <&kp>, <&caps_word>;
        };
    };
    macros {
        win_sleep: win_sleep {
            wait-ms = <50>;
            tap-ms = <5>;
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LG(X) &kp U &kp S>;
        };
    };
    keymap {
        compatible = "zmk,keymap";
        layer0 {
            display-name = "QWERTY";
            bindings = <
                 &kp Q            &kp W            &kp E            &kp R            &kp T                             &kp Y            &kp U            &kp I            &kp O            &kp P
                 &kp A            &kp S            &kp D            &kp F            &kp G                             &kp H            &kp J            &kp K            &kp L            &kp SEMI
                 &kp Z            &kp X            &kp C            &kp V            &kp B                             &kp N            &kp M            &kp COMMA        &kp DOT          &kp FSLH
                                                                    &lt 1 ESC        &lsk 3 LSHFT                      &kp SPACE        &lt 2 CAPS >;

        };
        layer1 {
            display-name = "NAVNUM";
            bindings = <
                 &kp TAB          &kp BSPC         &kp DEL          &kp HOME         &kp END                           &kp C_MUTE       &kp N9           &kp C_PREV       &kp C_NEXT       &kp C_PLAY_PAUSE
                 &kp ENTER        &hs LGUI LGUI    &hs LALT LALT    &hs LCTRL LCTRL  &kp PG_UP                         &kp C_VOL_UP     &kp N1           &kp N2           &kp N3           &kp N4
                 &kp LEFT         &kp UP           &kp DOWN         &kp RIGHT        &kp PG_DN                         &kp C_VOL_DN     &kp N5           &kp N6           &kp N7           &kp N8
                                                                    &none            &none                             &hcw LSHFT CAPS  &kp N0 >;

        };
        layer2 {
            display-name = "SYMBOL";
            bindings = <
                 &kp EXCL         &kp AT           &kp POUND        &kp DLLR         &kp PRCNT                         &kp CARET        &kp AMPS         &kp ASTRK        &kp MINUS        &kp PLUS
                 &kp SQT          &kp LBRC         &kp LBKT         &kp LPAR         &kp GRAVE                         &kp BSLH         &hs LCTRL LCTRL  &hs LALT LALT    &hs LGUI LGUI    &kp COLON
                 &kp DQT          &kp RBRC         &kp RBKT         &kp RPAR         &kp TILDE                         &kp PIPE         &kp UNDER        &kp LT           &kp GT           &kp QMARK
                                                                    &kp EQUAL        &hs LSHFT LSHFT                   &none            &none >;

        };
        layer3 {
            display-name = "FUNMSE";
            bindings = <
                 &kp F12          &kp F11          &kp F10          &kp F9           &kp SPACE                         &kp INS          &mmv MOVE_LEFT   &mmv MOVE_DOWN   &mmv MOVE_UP     &mmv MOVE_RIGHT
                 &kp F4           &kp F3           &kp F2           &kp F1           &kp GRAVE                         &kp K_APP        &hmk LCTRL LCLK  &hmk LALT RCLK   &hmk LGUI MB4    &hmk LSHFT MB5
                 &kp F8           &kp F7           &kp F6           &kp F5           &kp TILDE                         &win_sleep       &msc SCRL_LEFT   &msc SCRL_DOWN   &msc SCRL_UP     &msc SCRL_RIGHT
                                                                    &none            &none                             &lsl 4 4         &mkp MB3 >;

        };
        layer4 {
            display-name = "CONFIG";
            bindings = <
                 &bt BT_SEL 0     &bt BT_SEL 1     &bt BT_SEL 2     &bt BT_SEL 3     &bt BT_SEL 4                      &none            &none            &none            &none            &none
                 &bt BT_DISC 0    &bt BT_DISC 1    &bt BT_DISC 2    &bt BT_DISC 3    &bt BT_DISC 4                     &none            &none            &none            &none            &none
                 &bt BT_CLR_ALL   &bt BT_PRV       &bt BT_NXT       &bt BT_CLR       &default_report                   &out OUT_TOG     &none            &none            &none            &none
                                                                    &bootloader      &none                             &none            &studio_unlock >;

        };
    };
};
